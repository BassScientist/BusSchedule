<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.example">
  <class name="BusSchedule" table="bus_timetable">
    <id name="id">
      <generator class="native"/>
    </id>
    <property name="departureTime" column="departure_time"/>
    <property name="arrivalTime" column="arrival_time"/>
    <property name="price"/>
    <property name="originCity" column="origin_city"/>
    <property name="destinationCity" column="destination_city"/>
  </class>
</hibernate-mapping>



